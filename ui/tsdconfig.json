{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "rootDir": "./out/", 
    "outDir": "./out/",
    "paths": {
      "@/*": [
        "./out/*"
      ],
			"@ui-root/*": ["./out/*"]
    }
  },
  "include": [
    "out/**/*.ts",
    "out/**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
