{
  "name": "@thestarweb/ui",
  "version": "1.6.1-rebuild",
  "description": "A UI lib",
  "scripts": {
    "build": "vue-cli-service build --target lib --name star-ui --dest dist/ src/main.ts",
    "tsc": "node ./build-ts/main.js&&tsc --p ./tsdconfig.json"
  },
  "author": "star_ss",
  "bugs": {
    "url": "https://github.com/thestarweb/star-ui/issues"
  },
  "homepage": "https://ui.thestarweb.cn",
  "keywords": [
    "UI",
    "Vue",
    "Vue3",
    "TypeScript"
  ],
  "license": "MIT",
  "main": "dist/star-ui.umd.min.js",
  "types": "types/main.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thestarweb/star-ui.git"
  },
  "files": [
    "src",
    "dist",
    "types",
    "build-ts"
  ],
  "dependencies": {
    "core-js": "^3.15.2",
    "moment": "^2.29.1",
    "vue-class-component": "^8.0.0-0"
  },
  "peerDependencies": {
    "vue": "^3.1.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-typescript": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.1.5",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.14.0",
    "typescript": "~4.1.6",
